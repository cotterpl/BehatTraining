<table class="table table-striped table-hover table-movie-list">
    <thead>
    <tr>
        <th>title</th>
        <th>year</th>
    </tr>
    </thead>
    <tbody>
    {% for movie in movieList %}
        <tr class="tr-movie">
            <td><a href="/movie/{{ movie.id }}">{{ movie.title }}</a></td>
            <td>{{ movie.year }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
